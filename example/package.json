{
  "name": "bitmovin-player-react-native-example",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "pods": "yarn pods-install || yarn pods-update",
    "pods-install": "[ \"$(uname)\" != Darwin ] || NO_FLIPPER=1 yarn pod-install",
    "pods-update": "[ \"$(uname)\" != Darwin ] || cd ios && NO_FLIPPER=1 pod update --silent"
  },
  "dependencies": {
    "@react-native-tvos/config-tv": "^0.1.1",
    "expo": "~52.0.11",
    "react": "18.3.1",
    "react-native": "npm:react-native-tvos@0.76.3-0",
    "expo-build-properties": "~0.13.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "6.1.14",
    "@react-navigation/native-stack": "6.9.22",
    "react-native-modal": "13.0.1",
    "react-native-orientation-locker": "1.7.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.1.0",
    "react-native-system-navigation-bar": "^2.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3",
    "pod-install": "^0.1.0"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    },
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
