{
  "name": "Bitmovin Player React Native SDK",
  "out": "./docs/generated",
  "tsconfig": "./tsconfig.json",
  "excludeProtected": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "includeVersion": true,
  "entryPoints": ["./src"],
  "exclude": ["./example/**/*", "./integration_test/**/*"],
  "treatWarningsAsErrors": true,
  "blockTags": [
    "@platform",
    "@remarks",
    "@see",
    "@param",
    "@returns",
    "@example",
    "@deprecated",
    "@defaultValue",
    "@typeParam"
  ]
}
